   95.                CALL iom_nf90_check(NF90_OPEN( TRIM(cdname), NF90_NOWRITE, if90id, chunksize = ichunk ), clinfo)
   96.             ENDIF
   97.          ELSE                                       ! the file does not exist (or we overwrite it)
   98.             !                 ! =============
