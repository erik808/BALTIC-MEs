  373.          IF( numdct_vol      /= -1 )   CLOSE( numdct_vol      )   ! volume transports
  374.          IF( numdct_heat     /= -1 )   CLOSE( numdct_heat     )   ! heat transports
  375.          IF( numdct_salt     /= -1 )   CLOSE( numdct_salt     )   ! salt transports
